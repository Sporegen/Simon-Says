    								</VisualState>
    							</VisualStateGroup>
    						</VisualStateManager.VisualStateGroups>
    						<ed:RegularPolygon x:Name="regularPolygon" Fill="#FFA44040" InnerRadius="1" PointCount="6" Stretch="Fill" Stroke="Black" RenderTransformOrigin="0.5,0.5">
